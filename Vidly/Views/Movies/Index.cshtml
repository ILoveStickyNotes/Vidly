
@model IEnumerable<Vidly.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Movies</h1>

<table class ="table table-bordered table-hover" style="width: 100%">
    <tr>
        <th>Movie</th>
    </tr>

    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Name</td>
            <td>@movie.GenreType.Genre</td>
        </tr>
    }

</table>